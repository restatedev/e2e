{
  "bundles": [
    {
      "bundleName": "Apache-2.0",
      "licenseName": "Apache License, Version 2.0",
      "licenseUrl": "https://www.apache.org/licenses/LICENSE-2.0"
    },
    {
      "bundleName": "MIT",
      "licenseName": "The MIT License (MIT)"
    }
  ],
  "transformationRules": [
    {
      "bundleName": "Apache-2.0",
      "licenseNamePattern": ".*The Apache Software License, Version 2\\.0.*"
    },
    {
      "bundleName": "Apache-2.0",
      "licenseNamePattern": "Apache[ |-|_]2.*"
    },
    {
      "bundleName": "Apache-2.0",
      "licenseNamePattern": "ASL 2\\.0"
    },
    {
      "bundleName": "Apache-2.0",
      "licenseNamePattern": ".*Apache License,?( Version)? 2.*"
    },
    {
      "bundleName": "MIT",
      "licenseNamePattern": ".*MIT License.*"
    },
    {
      "bundleName": "Apache-2.0",
      "modulePattern": "org.jetbrains.kotlinx:kotlinx-serialization-json:.*"
    },
    {
      "bundleName": "Apache-2.0",
      "modulePattern": "org.jetbrains.kotlinx:kotlinx-serialization-core:.*"
    },
    {
      "bundleName": "Apache-2.0",
      "modulePattern": "com.squareup.okio:okio:.*"
    }
  ]
}